{
  "name": "@aiu/agents",
  "version": "0.1.0",
  "description": "Agent runtime for autonomous coding agents (Claude Code, Gemini CLI, Codex)",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src"
  },
  "dependencies": {
    "@aiu/core": "workspace:*",
    "@aiu/observability": "workspace:*",
    "micromatch": "^4.0.8",
    "pino": "^9.7.0"
  },
  "devDependencies": {
    "@types/micromatch": "^4.0.9",
    "@types/node": "^22.10.5",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "keywords": ["ai", "agents", "coding", "automation"],
  "license": "MIT"
}
