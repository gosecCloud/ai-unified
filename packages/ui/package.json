{
  "name": "@aiu/ui",
  "version": "0.1.0",
  "description": "Headless React UI components for AI Unified",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "lint": "eslint src"
  },
  "peerDependencies": {
    "react": "^18.0.0"
  },
  "dependencies": {
    "@aiu/core": "workspace:*"
  },
  "devDependencies": {
    "@types/react": "^18.2.48",
    "react": "^18.2.0"
  },
  "keywords": ["react", "ui", "components", "headless"],
  "license": "MIT"
}
